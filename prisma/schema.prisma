generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Attendance {
  id        Int      @id @default(autoincrement())
  date      DateTime
  subject   String
  status    String
  method    String
  createdAt DateTime @default(now())
}
